<launch>

<rosparam file="$(find 2FG7_v1_description)/launch/controller.yaml" command="load"/>
<node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen" ns="2FG7_v1" args="top_to_left_finger_position_controller joint_state_controller "/>
<node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="false" output="screen">
  <remap from="/joint_states" to="/2FG7_v1/joint_states"/>
</node>

</launch>